<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!-- 
        Croatia
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="112" Category="0" Condition="2" />
    <EccEntry Ecc="192" Category="0" Condition="2" />
    <EccEntry Ecc="194" Category="0" Condition="2" />
    <EccEntry Ecc="193" Category="0" Condition="2" />
    <EccEntry Ecc="195" Category="0" Condition="2" />
    <EccEntry Ecc="1987" Category="0" Condition="2" />
</EccTable>